{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key",
    "GEMINI_MODEL": "gemini-2.0-flash",
    "GEMINI_PROVIDER": "google",
    "MOCK_RESPONSE": "false"
  }
}
